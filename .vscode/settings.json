{
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "string",
        "settings": {
          "foreground": "#e6925b" // naranja fuerte
        }
      },
      {
        "scope": ["entity.name.function", "support.function"],
        "settings": {
          "foreground": "#ffe15e" // amarillo intenso
        }
      },
      {
        "scope": ["variable", "meta.definition.variable"],
        "settings": {
          "foreground": "#80d7ff"
        }
      },
      {
        "scope": [
          "keyword.control",
          "keyword.control.conditional",
          "keyword.control.loop",
          "keyword.control.switch",
          "keyword.control.trycatch",
          "keyword.control.return"
        ],
        "settings": {
          "foreground": "#C084FC"
        }
      }
    ]
  },
  "workbench.colorCustomizations": {
    "editor.background": "#0b0f1a",
    "editor.foreground": "#E6EDF3",
    "sideBar.background": "#0b1220",
    "activityBar.background": "#0f182b",
    "statusBar.background": "#1c52c7",
    "titleBar.activeBackground": "#0f182b",
    "tab.activeBackground": "#0b0f1a",
    "tab.inactiveBackground": "#0f182b",
    "tab.activeForeground": "#E6EDF3",
    "tab.inactiveForeground": "#7f8ea3",
    "tab.hoverBackground": "#14203a",
    "editorGroupHeader.tabsBackground": "#0f182b"
  }
}
